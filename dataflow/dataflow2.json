{
	"name": "dataflow2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_target_parquet",
						"type": "DatasetReference"
					},
					"name": "RawCompetencia"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_source_parquet_master",
						"type": "DatasetReference"
					},
					"name": "Output"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          ID_ORGANIZATION as string,",
				"          CPE_ID_VARIABLE as string,",
				"          CPE_NM_VARENG as string,",
				"          CPE_NM_VARFRA as string,",
				"          CPE_NM_VARESP as string,",
				"          DT_START as string,",
				"          DT_END as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> RawCompetencia",
				"RawCompetencia sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     filePattern:'master_detalle_comp_cola[n].parquet',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> Output"
			]
		}
	}
}