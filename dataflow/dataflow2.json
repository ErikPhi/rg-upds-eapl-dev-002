{
	"name": "dataflow2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_target_parquet",
						"type": "DatasetReference"
					},
					"name": "RawCompetencia"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_source_parquet_master",
						"type": "DatasetReference"
					},
					"name": "Output"
				}
			],
			"transformations": [
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ID_ORGANIZATION as string,",
				"          SCO_ID_HR as string,",
				"          SCO_OR_HR_ROLE as string,",
				"          SCO_DT_START_EVAL as string,",
				"          STD_ID_HR as string,",
				"          CPE_ORDINAL as string,",
				"          CPE_ID_ETAPA as string,",
				"          CPE_ID_VARIABLE as string,",
				"          SCO_ID_LEVEL as string,",
				"          SCO_VALUE_RAT as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> RawCompetencia",
				"RawCompetencia select(mapColumn(",
				"          ID_ORGANIZATION,",
				"          SCO_ID_HR,",
				"          SCO_DT_START_EVAL,",
				"          CPE_ID_VARIABLE,",
				"          SCO_ID_LEVEL,",
				"          SCO_VALUE_RAT",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     filePattern:'master_detalle_comp_cola[n].parquet',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> Output"
			]
		}
	}
}